{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "index.js",
      "README.md",
      "data-structures",
      "algorithms",
      "utils"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "template": "./node_modules/clean-jsdoc-theme",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "theme_opts": {
      "default_theme": "dark",
      "search": true,
      "title": "UsingBytes.com",
      "base_url": "https://usingbytes.com/"
    }
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "useLongnameInNav": true
    }
  },
  "metadata": {
    "title": "usingbytes-dsa"
  },
  "markdown": {
    "idInHeadings": true
  }
}
